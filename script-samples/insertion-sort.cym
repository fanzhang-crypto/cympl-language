#! cympl interpret

func insertionSort(arr:INT[], n:INT):INT[] {
    for(i:INT = 1; i < n; i = i + 1) {
        key:INT = arr[i];
        j:INT = i - 1;

        /* Move elements of arr[0..i-1], that are
           greater than key, to one position ahead
           of their current position */
        while (j >= 0 && arr[j] > key) {
            arr[j+1] = arr[j];
            j = j - 1;
        }
        arr[j+1] = key;
    }
    return arr;
}

insertionSort([2,3,9,1,11,32,17,23,15,21], 10);
